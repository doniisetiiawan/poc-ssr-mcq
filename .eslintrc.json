{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "no-console": "off",
    "react/jsx-one-expression-per-line": "off",
    "no-param-reassign": "off",
    "react/prop-types": "off",
    "no-shadow": "off",
    "no-underscore-dangle": "off"
  }
}
